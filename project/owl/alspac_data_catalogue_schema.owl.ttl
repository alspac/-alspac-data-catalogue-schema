linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

alspac_data_catalogue_schema:NamedAlspacDataSetCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "named_alspac_data_setCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom alspac_data_catalogue_schema:NamedAlspacDataSet ;
            owl:onProperty alspac_data_catalogue_schema:entries ] ;
    skos:definition "A holder for named_alspac_data_set objects" .

alspac_data_catalogue_schema:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" .

alspac_data_catalogue_schema:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    rdfs:range alspac_data_catalogue_schema:NamedAlspacDataSet .

alspac_data_catalogue_schema:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range alspac_data_catalogue_schema:PersonStatus ;
    skos:definition "living or dead status" .

PATO:0001421 a owl:Class,
        alspac_data_catalogue_schema:PersonStatus ;
    rdfs:label "ALIVE" .

PATO:0001422 a owl:Class,
        alspac_data_catalogue_schema:PersonStatus ;
    rdfs:label "DEAD" .

schema:birthDate a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:exactMatch schema:birthDate .

schema:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable description for a thing" ;
    skos:exactMatch schema:description .

schema:email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    rdfs:range linkml:String ;
    skos:definition "The main email address of a person" ;
    skos:exactMatch schema:email .

schema:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:exactMatch schema:identifier .

schema:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable name for a thing" ;
    skos:exactMatch schema:name .

alspac_data_catalogue_schema:NamedAlspacDataSet a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "named_alspac_data_set" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass alspac_data_catalogue_schema:PersonStatus ;
            owl:onProperty alspac_data_catalogue_schema:vital_status ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty alspac_data_catalogue_schema:age_in_years ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:email ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Date ;
            owl:onProperty schema:birthDate ],
        alspac_data_catalogue_schema:NamedThing ;
    skos:definition "Represents a named_alspac_data_set" .

alspac_data_catalogue_schema:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:description ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema:Thing .

<https://w3id.org/alspac/alspac-data-catalogue-schema/PersonStatus#UNKNOWN> a owl:Class,
        alspac_data_catalogue_schema:PersonStatus ;
    rdfs:label "UNKNOWN" .

