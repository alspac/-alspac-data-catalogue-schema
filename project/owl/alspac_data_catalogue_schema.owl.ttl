    rdfs:label "class_definition" .

alspac_data_catalogue_schema:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "PersonStatus" ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/alspac/alspac-data-catalogue-schema/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/alspac/alspac-data-catalogue-schema/PersonStatus#UNKNOWN> .

