  <rdf:Description rdf:about="http://purl.org/alspac/alspac-data-catalogue-schema/c20cd22a-61ac-49a6-8adb-2b877868784f">
    <dcat:mediaType>.fam</dcat:mediaType>
    <dcat:byteSize>249k</dcat:byteSize>
    <schema:name>freeze_id.fam</schema:name>
    <schema:description>A text file with no header line, and one line per sample with the following six fields:
  1. Family ID ('FID')
  2. Within-family ID ('IID'; cannot be '0')
  3. Within-family ID of father ('0' if father isn't in dataset)
  4. Within-family ID of mother ('0' if mother isn't in dataset)
  5. Sex code ('1' = male, '2' = female, '0' = unknown)
  6. Phenotype value ('1' = control, '2' = case,
  '-9'/'0'/non-numeric =
  missing data if case/control)</schema:description>
    <alspacdcs:number_of_participants rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8224</alspacdcs:number_of_participants>
