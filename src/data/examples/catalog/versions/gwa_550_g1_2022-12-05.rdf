  <rdf:Description rdf:about="http://purl.org/alspac/alspac-data-catalogue-schema/fb0184f4-82b9-472f-86c4-fc3de9986b6c">
    <dcat:mediaType>.fam</dcat:mediaType>
    <dcat:byteSize>139k</dcat:byteSize>
    <schema:name>data.fam</schema:name>
    <schema:description>A text file with no header line, and one line per sample with the following six fields:
  1. Family ID ('FID')
  2. Within-family ID ('IID'; cannot be '0')
  3. Within-family ID of father ('0' if father isn't in dataset)
  4. Within-family ID of mother ('0' if mother isn't in dataset)
  5. Sex code ('1' = male, '2' = female, '0' = unknown)
  6. Phenotype value ('1' = control, '2' = case,
  '-9'/'0'/non-numeric =
  missing data if case/control)</schema:description>
    <alspacdcs:md5sum>171c7c4b5877b539a509328a03b8ce35</alspacdcs:md5sum>
    <alspacdcs:belongs_to_container rdf:resource="http://purl.org/alspac/alspac-data-catalogue-schema/69b0b1a9-9af0-4b85-a9c9-2973fc1ab5c4"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/alspac/alspac-data-catalogue-schema/gwa_550_g1_2022-12-05_f2">
